{
  "name": "tomcizek/symfony-prooph",
  "type": "library",
  "description": "Symfony Prooph Bundle which allows you to configure prooph components through yml symfony config.",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomáš Čížek",
      "email": "tomascizek@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/container": "^1.0",
    "prooph/service-bus": "^6.0",
    "prooph/event-store": "^7.2",
    "prooph/pdo-event-store": "^1.5",
    "prooph/event-store-bus-bridge": "^3.0",
    "prooph/event-sourcing": "^5.2",
    "proophsoftware/prooph-interop-bundle": "^0.1.1",
    "sandrokeil/interop-config": "^2.1"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "phpstan/phpstan": "^0.8.5",
    "mockery/mockery": "^0.9.9",
    "react/promise": "^2.5",
    "symfony/dependency-injection": "^3.3",
    "symfony/yaml": "^3.3",
    "symfony/expression-language": "^3.3"
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  }
}
